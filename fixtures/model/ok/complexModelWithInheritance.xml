<?xml version="1.0" encoding="UTF-8"?>
<root model-name="complex model #4">
	<models>
		<model name="person">
			<property type="string" name="firstname"/>
			<property type="string" name="lastname"/>
			<property type="date" name="birthdate"/>
		</model>
		<model name="location">
			<property type="double" name="latitude"/>
			<property type="double" name="longitude"/>
		</model>
		<model name="place" inherit="location">
			<property type="string" name="name"/>
		</model>
		<model name="shop" inherit="place">
			<property type="person" name="owner"/>
			<property type="person[]" name="employees"/>
			<property type="person[]" name="customers"/>
		</model>
		<model name="company">
			<property type="person" name="ceo"/>
			<property type="shop[]" name="shops"/>
		</model>
	</models>
</root>