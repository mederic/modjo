<?xml version="1.0" encoding="UTF-8"?>
<root model-name="simple model #3">
	<models>
		<model name="location">
			<property type="double" name="latitude"/>
			<property type="double" name="longitude"/>
		</model>
		<model name="country">
			<!-- string to location map (java <=> Map<String, Location>) -->
			<property type="location[string]" name="cities"/>
		</model>	
	</models>
</root>