<?xml version="1.0" encoding="UTF-8"?>
<root model-name="iOS template #1">
    <equivalences>
        <equivalence name="ios">
            <string>NSString *</string>
            <date>NSDate *</date>
            <boolean>BOOL</boolean>
            <float>float </float>
            <int>int </int>
            <long>long </long>
            <double>double </double>
            <char>char </char>
            <model>Model *</model>
            <list>NSArray *</list>
            <map>NSDictionary *</map>
        </equivalence>
        <equivalence name="ios-property">
            <string>@property (nonatomic, strong) NSString *</string>
            <date>@property (nonatomic, strong) NSDate *</date>
            <boolean>@property (assign) BOOL </boolean>
            <float>@property (assign) float </float>
            <int>@property (assign) int </int>
            <long>@property (assign) long </long>
            <double>@property (assign) double </double>
            <char>@property (assign) char </char>
            <model>@property (nonatomic, strong) Model *</model>
            <list>@property (nonatomic, strong) NSArray *</list>
            <map>@property (nonatomic, strong) NSDictionary *</map>
        </equivalence>
    </equivalences>
    <templates>
        <template target="model">
	    <output name="{{Model}}.m" src="model.m.tmpl.txt"/>
	    <output name="{{Model}}.h" src="model.h.tmpl.txt"/>
	</template>
    </templates>
</root>
